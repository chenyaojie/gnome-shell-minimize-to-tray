{
  "compilerOptions": {
    "lib": ["ES2017"],
    "target": "ES2017",
    "strict": true,
    "noImplicitAny": false,
    "strictNullChecks": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "baseUrl": "./src/",
    "moduleResolution": "Node",
    "paths": {
      "@imports/*": ["../@types/Gjs/*"],
      "@mtt/*": ["./*"]
    }
  }
}
